{
  "name": "heb-takehome",
  "version": "1.0.0",
  "description": "H-E-B Interview Project: Front-End Pizza Ordering",
  "nodemonConfig": {
    "ignore": [
      "dist/",
      ".parcel-cache",
      "node_modules",
      ".vscode"
    ]
  },
  "scripts": {
    "pre-clean": "rimraf .parcel-cache & rimraf dist",
    "start": "pre-clean & parcel",
    "build": "parcel build",
    "dev": "npm run pre-clean & concurrently --kill-others \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "parcel watch public/index.html",
    "dev:server": "nodemon --inspect --verbose server/index.js",
    "lint": "standard --verbose | snazzy",
    "lint-staged": "lint-staged",
    "lint:fix": "standard --fix --verbose | snazzy",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scottiea2k11/HEB-TakeHome.git"
  },
  "keywords": [
    "node"
  ],
  "author": "raymond ayala",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scottiea2k11/HEB-TakeHome/issues"
  },
  "homepage": "https://github.com/scottiea2k11/HEB-TakeHome#readme",
  "dependencies": {
    "axios": "^0.27.2",
    "config": "^3.3.7",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "momentjs": "^2.0.0",
    "parcel-bundler": "^1.12.5",
    "path": "^0.12.7",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^8.0.2",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.1",
    "styled-components": "^5.3.5",
    "styled-normalize": "^8.0.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@reduxjs/toolkit": "^1.8.3",
    "body-parser": "^1.20.0",
    "buffer": "^6.0.3",
    "concurrently": "^7.3.0",
    "eslint": "^8.20.0",
    "events": "^3.3.0",
    "http-proxy-middleware": "^2.0.6",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.19",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "pre-commit": "^1.2.2",
    "rimraf": "^3.0.2",
    "router": "^1.3.7",
    "snazzy": "^9.0.0",
    "util": "^0.12.4"
  }
}
